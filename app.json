{
  "expo": {
    "name": "Quản lý thương vụ XMTĐ",
    "slug": "Auditapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-adaptive.png",
    "scheme": "auditapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-adaptive.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "enableProguardInReleaseBuilds": true,
      "allowBackup": false,
      "package": "com.hung141525.Auditapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon-adaptive.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fefefe",
          "dark": {
            "backgroundColor": "#fefefe"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Ứng dụng cần quyền truy cập camera để chụp ảnh audit cửa hàng."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Ứng dụng cần quyền truy cập vị trí để ghi nhận thông tin địa điểm audit."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1ea6010a-a487-4e40-a333-74886ee6d4aa"
      }
    },
    "owner": "xmtd"
  }
}
